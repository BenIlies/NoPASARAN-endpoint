SERVER_HOST=www.nopasaran.org
SERVER_PORT=443
SERVER_DOMAIN_NAME=nopasaran.org

API_GENERATE_SSH_CERTIFICATE_PATH=/api/v1/generate-ssh-certificate
API_GET_NODE_CA_PATH=/api/v1/node-certificate-authority
API_GET_POOL_MANAGER_CA_PATH=/api/v1/pool-manager-certificate-authority
API_GET_X509_CA_PATH=/api/v1/x509-certificate-authority

USER_INPUT_PATH=~/.ssh/
USER_INPUT_FILENAME_PUBLIC=ssh_user_rsa_key.pub
USER_INPUT_FILENAME_PRIVATE=ssh_user_rsa_key
USER_GENERATE=1
USER_OUTPUT_PATH=~/.ssh/
USER_OUTPUT_FILENAME=ssh_user_rsa_key-cert
USER_SSHD_CONFIG_PATH=/etc/ssh/sshd_config

HOST_INPUT_PATH=/etc/ssh
HOST_INPUT_FILENAME_PUBLIC=ssh_host_rsa_key.pub
HOST_INPUT_FILENAME_PRIVATE=ssh_host_rsa_key
HOST_GENERATE=1
HOST_OUTPUT_PATH=/etc/ssh/
HOST_OUTPUT_FILENAME=ssh_host_rsa_key-cert.pub
HOST_SSHD_CONFIG_PATH=/etc/ssh/sshd_config

OTHER_CA_OUTPUT_PATH=/etc/ssh
OTHER_CA_OUTPUT_FILENAME=other-ca.pub
OTHER_CA_KNOWN_HOST_PATH=~/.ssh
OTHER_CA_KNOWN_HOST_FILENAME=known_hosts
OTHER_CA_DOMAIN_SUFFIX=nopasaran.org

OWN_CA_OUTPUT_PATH=/etc/ssh
OWN_CA_OUTPUT_FILENAME=own-ca.pub
OWN_CA_SSH_CLIENT_CONFIG_PATH=~/.ssh/config
OWN_CA_DOMAIN_SUFFIX=nopasaran.org

X509_CA_OUTPUT_PATH=
X509_CA_OUTPUT_FILENAME=ca.pem
